{
  "name": "nyxbi",
  "type": "module",
  "version": "0.0.17",
  "description": "⚡️ Nyxb Generation CLI Experience",
  "license": "MIT",
  "repository": "nyxb/cli",
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./bin/nyxbi.mjs"
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "nyxbi": "./bin/nyxbi.mjs",
    "nyxbi-ng": "./bin/nyxbi.mjs",
    "nyxb": "./bin/nyxbi.mjs",
    "nyxb-cli": "./bin/nyxbi.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "build:stub": "unbuild --stub",
    "dev": "node ./bin/nyxbi.mjs dev ./playground",
    "dev:bun": "bun --bun ./bin/nyxbi.mjs dev ./playground",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "nyxbi": "node ./bin/nyxbi.mjs",
    "nyxbi-bun": "bun --bun ./bin/nyxb.mjs",
    "prepack": "unbuild",
    "release": "pnpm test && npx changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && pnpm build",
    "test:types": "tsc --noEmit"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.8.0",
    "@nyxb/eslint-config": "1.0.0-beta.12",
    "@types/node": "^20.8.8",
    "@types/semver": "^7.5.4",
    "citty": "^0.1.4",
    "consola": "^3.2.3",
    "eslint": "^8.52.0",
    "execa": "^8.0.1",
    "giget": "^1.1.3",
    "nypm": "^0.3.3",
    "pathe": "^1.1.1",
    "semver": "^7.5.4",
    "std-env": "^3.4.3",
    "unbuild": "^2.0.0",
    "ws": "^8.14.2"
  }
}